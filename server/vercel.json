{
  "version": 2,
  "builds": [
    {
      "src": "emapi/app.js",
      "use": "@vercel/node"
    }, 
    {
      "src": "notapi/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "ratapi/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/emapi/(.*)",
      "dest": "/emapi/"
    },
    {
      "src": "/notapi/(.*)",
      "dest": "/notapi/app.js"
    },
    {
      "src": "ratapi/(.*)",
      "dest": "/ratapi/app.js"
    }
  ]
}
